{
  "name": "react-screenshots",
  "version": "0.1.2",
  "description": "a picture clipping and graffiti tool by react",
  "main": "dist/screenshots.js",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "dev": "vite",
    "tsc": "tsc --watch",
    "build": "tsc && vite build",
    "lint": "eslint src --ext .js,.ts,.jsx,.tsx --fix",
    "serve": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nashaofu/screenshots.git"
  },
  "keywords": [
    "clipping",
    "graffiti",
    "react",
    "image"
  ],
  "author": "nashaofu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nashaofu/screenshots/issues"
  },
  "homepage": "https://github.com/nashaofu/screenshots/tree/master/packages/react-screenshots#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "@vitejs/plugin-react": "^1.1.0",
    "electron": "^16.0.4",
    "eslint": "^8.4.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-jsx": "^10.0.0",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "less": "^4.1.2",
    "typescript": "^4.5.2",
    "vite": "^2.6.14"
  }
